@model StoreFront.DATA.Shipper

@{
    ViewBag.Title = "Shipping Partner Details";
}

<div class="container py-5">
    <h2>@ViewBag.Title</h2>

    <div>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.Name)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Name)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.ContactFirst)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.ContactFirst)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.ContactLast)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.ContactLast)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Email)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Email)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Phone)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Phone)
            </dd>

        </dl>
    </div>
    <p>
        @Html.ActionLink("Edit", "Edit", new { id = Model.ShipperID }) |
        @Html.ActionLink("Back to Shipping Partners", "Index")
    </p>
</div>
